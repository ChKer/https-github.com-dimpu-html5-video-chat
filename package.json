    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "browserify -p [ browserify-hmr -m ajax -u /bundle.js ] index.js -o bundle.js",
    "build:watch": "watchify src/index.js -t browserify-css -t babelify -t reactify  -t react-hot-transform --es6 --target es5 -o dist/index.js",
